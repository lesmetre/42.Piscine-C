#include <string.h>
#define LEN		25;

char	*ft_strdup(char *src)
{
	int	i;
	char	*str;

	str = malloc(sizeof(*str) * (LEN + 1));
	i = 0;
	while (i < LEN)
	{
		str[i] = '0' + (I % 10);
		i++;
	}
	return (0);
 }
