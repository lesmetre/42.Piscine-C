#include <unistd.h>

void    ft_putchar(char c);

void    ft_putstr(char *str)
{
    int stri;
    stri = 0;
    while (str[stri])
    {
        ft_putchar(str[stri]);
        stri++;
    }
}

int     main(int argc, char **argv)
{
    argc = 0;

    ft_putstr(argv[0]);
    ft_putchar('\n');
    return (0);
}
